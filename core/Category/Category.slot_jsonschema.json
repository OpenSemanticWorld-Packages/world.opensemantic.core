        }
    ],
    "type": "object",
    "title": "Category",
    "title*": {
        "de": "Kategorie",
        "en": "Category"
    },
    "allOf": [
        {
            "type": "object",
            "$ref": "/wiki/Category:Entity?action=raw&slot=jsonschema"
        }
    ],
    "required": [
        "subclass_of"
    ],
    "properties": {
        "subclass_of": {
            "title": "Supercategories",
            "title*": {
                "en": "Supercategories",
                "de": "\u00dcberkategorien"
